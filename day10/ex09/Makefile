NAME= ft_advanced_do-op

SRC= calc_funs.c main.c print_fun.c str_fun.c

all: $(NAME)

$(NAME):
	@gcc -o $(NAME) $(SRC)

clean:
	@/bin/rm -f *.gch
	@/bin/rm -f $(NAME)
